{
  "name": "mimmi20/mezzio-navigation-laminasviewrenderer-bootstrap",
  "description": "Provides viewhelper for Mezzio Navigation to use with Bootstrap",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/mezzio-navigation-laminasviewrenderer-bootstrap",
  "support": {
    "issues": "https://github.com/mimmi20/mezzio-navigation-laminasviewrenderer-bootstrap/issues",
    "source": "https://github.com/mimmi20/mezzio-navigation-laminasviewrenderer-bootstrap"
  },
  "require": {
    "php": "~8.3.0 || ~8.4.0 || ~8.5.0",
    "laminas/laminas-stdlib": "^3.20.0",
    "laminas/laminas-view": "^2.39.0",
    "mimmi20/laminasviewrenderer-helper-htmlelement": "^3.0.0",
    "mimmi20/laminasviewrenderer-helper-partialrenderer": "^3.0.0",
    "mimmi20/mezzio-navigation": "^4.0.3",
    "mimmi20/mezzio-navigation-laminasviewrenderer": "^6.0.0",
    "mimmi20/navigation-helper-containerparser": "^5.1.0",
    "mimmi20/navigation-helper-htmlify": "^5.0.2",
    "psr/container": "^1.1.2 || ^2.0.2"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-intl": "*",
    "ext-mbstring": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.29.14",
    "laminas/laminas-escaper": "^2.17.0",
    "laminas/laminas-i18n": "^2.30.0",
    "laminas/laminas-permissions-acl": "^2.17.0",
    "laminas/laminas-servicemanager": "^3.23.0",
    "mezzio/mezzio-authentication": "^1.11.0",
    "mezzio/mezzio-helpers": "^5.18.0",
    "mezzio/mezzio-laminasviewrenderer": "^2.18.0",
    "mezzio/mezzio-router": "^3.18.0",
    "mimmi20/coding-standard": "^6.0.29",
    "mimmi20/mezzio-generic-authorization": "^4.0.0",
    "mimmi20/mezzio-generic-authorization-acl": "^4.0.1",
    "mimmi20/navigation-helper-converttopages": "^3.0.0",
    "mimmi20/navigation-helper-findroot": "^3.0.0",
    "nikic/php-parser": "^5.4.0",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^2.1.14",
    "phpstan/phpstan-deprecation-rules": "^2.0.2",
    "phpunit/phpunit": "^12.1.5",
    "psr/http-server-middleware": "^1.0.2",
    "rector/rector": "^2.0.15",
    "rector/type-perfect": "^2.1.0",
    "shipmonk/composer-dependency-analyser": "^1.8.2",
    "symfony/process": "^7.2.5",
    "symplify/phpstan-rules": "^14.6.9",
    "tomasvotruba/cognitive-complexity": "^1.0.0",
    "tomasvotruba/type-coverage": "^2.0.2",
    "tomasvotruba/unused-public": "^2.0.1"
  },
  "suggest": {
    "ext-intl": "required to use the Translator",
    "laminas/laminas-config": "^3.5.0, to provide page configuration (optional, as arrays and Traversables are also allowed)",
    "laminas/laminas-i18n": "^2.11.0, required to use the Translator",
    "laminas/laminas-servicemanager": "^3.6.4, to use the navigation factories",
    "mimmi20/mezzio-generic-authorization": "^1.0.1, to use authrization roles or privileges"
  },
  "autoload": {
    "psr-4": {
      "Mimmi20\\Mezzio\\Navigation\\LaminasView\\View\\Helper\\BootstrapNavigation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\Mezzio\\Navigation\\LaminasView\\View\\Helper\\BootstrapNavigation\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "config-provider": "Mimmi20\\Mezzio\\Navigation\\LaminasView\\View\\Helper\\BootstrapNavigation\\ConfigProvider"
    }
  }
}
