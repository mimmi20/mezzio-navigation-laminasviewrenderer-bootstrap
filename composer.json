{
  "name": "mimmi20/mezzio-navigation-laminasviewrenderer-bootstrap",
  "type": "library",
  "description": "Provides viewhelper for Mezzio Navigation to use with Bootstrap",
  "homepage": "https://github.com/mimmi20/mezzio-navigation-laminasviewrenderer-bootstrap",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4.0 || ^8.0.0",
    "ext-mbstring": "*",
    "mimmi20/mezzio-laminasviewrenderer-helpers": "^1.0.4",
    "mimmi20/mezzio-navigation": "^2.0.3",
    "mimmi20/mezzio-navigation-helpers": "^1.0.10",
    "mimmi20/mezzio-navigation-laminasviewrenderer": "^2.0.9"
  },
  "require-dev": {
    "ext-intl": "*",
    "infection/infection": "^0.21.5",
    "laminas/laminas-config": "^3.5.0",
    "laminas/laminas-escaper": "^2.7.0",
    "laminas/laminas-i18n": "^2.11.1",
    "laminas/laminas-servicemanager": "^3.6.4",
    "mezzio/mezzio-authentication": "^1.2.1",
    "mezzio/mezzio-router": "^3.3.0",
    "mimmi20/coding-standard": "^2.1.3",
    "mimmi20/mezzio-generic-authorization": "^2.0.1",
    "mimmi20/mezzio-generic-authorization-acl": "^2.0.1",
    "pepakriz/phpstan-exception-rules": "^0.11.6",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^0.12.85",
    "phpstan/phpstan-deprecation-rules": "^0.12.6",
    "phpstan/phpstan-phpunit": "~0.12.18",
    "phpunit/phpunit": "^9.5.4"
  },
  "suggest": {
    "ext-intl": "required to use the Translator",
    "laminas/laminas-config": "^3.5.0, to provide page configuration (optional, as arrays and Traversables are also allowed)",
    "laminas/laminas-i18n": "^2.11.0, required to use the Translator",
    "laminas/laminas-servicemanager": "^3.6.4, to use the navigation factories",
    "mimmi20/mezzio-generic-authorization": "^1.0.1, to use authrization roles or privileges"
  },
  "config": {
    "platform": {
      "php": "7.4.0"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "config-provider": "Mezzio\\Navigation\\LaminasView\\View\\Helper\\BootstrapNavigation\\ConfigProvider"
    }
  },
  "autoload": {
    "psr-4": {
      "Mezzio\\Navigation\\LaminasView\\View\\Helper\\BootstrapNavigation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MezzioTest\\Navigation\\LaminasView\\View\\Helper\\BootstrapNavigation\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/mimmi20/mezzio-navigation-laminasviewrenderer-bootstrap/issues",
    "source": "https://github.com/mimmi20/mezzio-navigation-laminasviewrenderer-bootstrap"
  }
}
